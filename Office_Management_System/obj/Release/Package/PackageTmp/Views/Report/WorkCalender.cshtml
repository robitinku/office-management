
<script>


    $(document).ready(function () {

        $("#save").on('click', function (e) {
            if ($("#Cell_Id").val() != "0") {
                var params = {
                    From_Date: $("#From_Date").val(),
                    To_Date: $("#To_Date").val(),
                    Cell_Id: $("#Cell_Id").val()
                };
                var tr = "";
                $.ajax({
                    url: '@Url.Action("EmpStatusCell")',
                    data: JSON.stringify({ _db_Order_Detail: params }),
                    type: 'POST',
                    dataType: 'json',
                    async: false,
                    contentType: 'application/json',
                    success: function (result) {

                        tr = result;

                    }
                });

                $('#child').html("");
                $('#child').append(tr);
            }

            else {

                alert("Employee Name Require")
            }

        });

       


         $("#From_Date").datepicker({

                dateFormat: "yy-mm-dd",
                changeYear: true,
                changeMonth: true,
                numberOfMonths: 1
            });

            $("#To_Date").datepicker({
                beforeShow: function () {
                    jQuery(this).datepicker('option', 'minDate', jQuery('#From_Date').val());
                },
                altFormat: "yy-mm-dd",
                dateFormat: "yy-mm-dd",
                changeYear: true,
                changeMonth: true,
                numberOfMonths: 1
            });
          


    });
    function valid() {
        ;
    }
</script>


