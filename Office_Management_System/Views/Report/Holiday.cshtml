@using Office_Dll;
@model List<db_calendar>

<div id="content">


    <div id="page-heading"><h1>List of All Employee</h1></div>
    

   
    <table border="0" width="100%" cellpadding="0" cellspacing="0" id="product-table">
        <tr>
            <th class="table-header-check" style="color:white">Sl No</th>
            <th class="table-header-repeat line-left minwidth-1"><a href=""> Date</a>	</th>
            <th class="table-header-repeat line-left minwidth-1"><a href="">Description</a></th>
        </tr>
       @{ int i = 1;}
        
        @foreach (var item in Model)
        { 
            
            <tr>
                <td>@i</td>
                <td>@Html.DisplayFor(modelItem => item.HoliDay_Date)</td>
                <td>@Html.DisplayFor(modelItem => item.Discription)</td>                
            </tr>
            i += 1;
        }
    </table>
   
    


<div class="clear">&nbsp;</div>

</div>
